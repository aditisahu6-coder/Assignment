#include <stdio.h>

int main() {
    int i, j;

    for (i = 1; i <= 5; i++) {
        // A
        for (j = 1; j <= 5; j++) {
            if (j == 1 || j == 5 || i == 1 || i == 3)
                printf("*");
            else
                printf(" ");
        }
        printf("  ");

        // D
        for (j = 1; j <= 5; j++) {
            if (j == 1 || (i == 1 && j < 5) || (i == 5 && j < 5) || (j == 5 && i != 1 && i != 5))
                printf("*");
            else
                printf(" ");
        }
        printf("  ");

        // I
        for (j = 1; j <= 5; j++) {
            if (i == 1 || i == 5 || j == 3)
                printf("*");
            else
                printf(" ");
        }
        printf("  ");

        // T
        for (j = 1; j <= 5; j++) {
            if (i == 1 || j == 3)
                printf("*");
            else
                printf(" ");
        }
        printf("  ");

        // I (again)
        for (j = 1; j <= 5; j++) {
            if (i == 1 || i == 5 || j == 3)
                printf("*");
            else
                printf(" ");
        }

        printf("\n");
    }

    return 0;
}
